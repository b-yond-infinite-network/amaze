{
  "name": "the-karaoke",
  "version": "1.0.0",
  "description": "A tool to fetch songs and lyrics",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack-dev-server",
    "tests": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "author": "Jonathan Massot",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "<rootDir>/src/tests/shim.js",
      "<rootDir>/src/tests/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@../(.*)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/src/tests/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@material-ui/icons": "^3.0.1",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.5",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/sinon": "^5.0.5",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-jsonp": "^1.1.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "node-sass": "^4.9.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0"
  }
}
